{
  "name": "sqlplus-runner",
  "displayName": "SQLPlus Runner",
  "publisher": "Faiztechnics",
  "description": "Takes the SQL commands written in VS Code and executes them in SQLPlus automatically",
  "icon": "Images/DBIcon.png",
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sqlplus-runner.executeInSQLPlus",
        "title": "Execute in SQLPlus"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "sqlplus-runner.executeInSQLPlus",
          "group": "navigation",
          "title": "Run SQLPlus",
          "icon": "Images/DBIcon.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
